INSERT INTO companies_contact (USR_MAJ, NOM, PRENOM, EMAIL, PHONE, ID_COMPANY, TYPE, BIKES_STATS) SELECT 'script', companies.LASTNAME_CONTACT_BILLING, companies.FIRSTNAME_CONTACT_BILLING, companies.EMAIL_CONTACT_BILLING, companies.PHONE_CONTACT_BILLING, companies.ID, 'billing', 'N' FROM companies WHERE companies.LASTNAME_CONTACT_BILLING IS NOT NULL




